<?xml version="1.0" encoding="UTF-8"?>
<Templates>
  <JimTank type = "SceneObjectTemplate">
    <Components>
      <Location type="LocationComponent">
        <AttachToParent value="1"/>
      </Location>
      <HullMesh type="MeshComponent">
        <Filename value="JIM_TANK_hull.mesh"/>
        <CastShadow value="1"/>
      </HullMesh>
      <HullCollision type="ODEGeometry">
        <GeometryType value="box"/>
      </HullCollision>
      <HullBody type="ODEBody">
        <Mass value="1.0"/>
      </HullBody>
      <EngineSound type="OpenALSoundComponent">
        <SoundFile value="sounds/vehicles/tank/engine.wav"/>
        <Loop value="1"/>
        <MinDistance value="5"/>
        <RolloffFactor value="1"/>
      </EngineSound>
      <EngineExhaustFumes type="ParticleSystemComponent">
        <ParticleTemplate value="ExhaustFumes"/>
      </EngineExhaustFumes>
      <Engine type="VehicleEngineComponent">
        <Wheels value="JimTankWheelR1 JimTankWheelR2 JimTankWheelR3 JimTankWheelR4 JimTankWheelR5 JimTankWheelR6 JimTankWheelL1 JimTankWheelL2 JimTankWheelL3 JimTankWheelL4 JimTankWheelL5 JimTankWheelL6"/>
        <EngineType value="tank"/>
        <Power value="0.02"/>
        <Automatic  value="1"/>
        <BrakeTorque  value="5"/>
        <DeclutchTimeChangeGear value="1.0"/>
        <ClutchTimeChangeGear value="1.0"/>
        <MaxRPM value="4000"/>
        <MinRPM value="500"/>
        <RPMGearChangeUp value="3000"/>
        <RPMGearChangeDown value="1500"/>
        <!--<GearRatio value="-13.42 -16.42 0 16.42 12.15 11.52 11.17"/>-->
        <GearRatio value="-13.42 -16.42 0 16.42 13.15 10.52 7.17"/>
        <TurnForce value="100"/>
      </Engine>
      <InputHandlerComponent type ="InputHandlerComponent">
        <ControlSetting value ="VehicleEngineInputSettings"/>
      </InputHandlerComponent>
      <HullArmor type ="ArmorComponent">
        <Armor value="50"/>
        <DamageMesh value="JIM_TANK_hull_demolished.mesh"/>
      </HullArmor>
    </Components>
    <ComponentContainers>
      <JimTankDriverCameraObject type ="SceneObjectTemplate">
        <Inheritance value ="JimTankCameraObject"/>
        <Components>
          <CameraLocation type="LocationComponent">
            <AttachToParent value ="1"/>
            <Position value="-1.0 1.0 -1.0"/>
            <Rotation value="0 0 0"/>
          </CameraLocation>
        </Components>
      </JimTankDriverCameraObject>
      <JimTankTurret type ="SceneObjectTemplate">
        <Components>
          <TurretLocation type="LocationComponent">
            <AttachToParent value="0"/>
            <Position value="0.0 0.45 0.7"/>
          </TurretLocation>
          <TurretMesh type="MeshComponent">
            <Filename value="turret.mesh"/>
            <CastShadow value="1"/>
          </TurretMesh>
          <TurretComp type="TurretComponent">
          </TurretComp>
          <TurretPhysicsGeom type="ODEGeometry">
            <GeometryType value="box"/>
          </TurretPhysicsGeom>
          <TurretPhysicsBody type="ODEBody">
            <Mass value="0.1"/>
          </TurretPhysicsBody>
          <TurretPhysicsJoint type="ODEJoint">
            <Type value="hinge"/>
            <HighStop value ="-1"/>
            <LowStop value ="1"/>
          </TurretPhysicsJoint>
          <TurretInput type ="InputHandlerComponent" >
            <ControlSetting value="TurretInputSettings"/>
          </TurretInput>
          <MainGun type ="WeaponSystemComponent">
            <ProjectileTemplate value="JimTankMainGunProjectileObject"/>
            <ProjectileStartOffset value="5.0"/>
            <ProjectileStartVelocity value="500"/>
            <NumberOfMagazines value="90"/>
            <MagazineSize value="90"/>
            <RecoilForce value="0 0 100"/>
          </MainGun>
        </Components>
        <ComponentContainers>
          <JimTankBarel type ="SceneObjectTemplate">
            <Components>
              <BarelLocation type="LocationComponent">
                <AttachToParent value="1"/>
                <Position value="0.0 0.3 -1.15"/>
              </BarelLocation>
              <BarelMesh type="MeshComponent">
                <Filename value="canon_bas.mesh"/>
                <CastShadow value="1"/>
              </BarelMesh>
            </Components>
          </JimTankBarel>
        </ComponentContainers>
      </JimTankTurret>
      <JimTankTrackL type ="SceneObjectTemplate">
        <Inheritance value ="JimTankTrack"/>
        <Components>
          <Track type="TrackComponent">
            <DriveWheel value="JimTankWheelL5"/>
            <AnimationSpeedFactor value="0.01 0"/>
          </Track>
          <TrackLocation type="LocationComponent">
            <Position value="1.2 0.0 0.0"/>
