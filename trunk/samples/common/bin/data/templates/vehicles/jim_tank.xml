  <SceneObjectTemplate>
    <Name value="JimTankTrack"/>
    <Components>
      <LocationComponent>
        <AttachToParent value="1"/>
      </LocationComponent>
      <MeshComponent>
        <Filename value="JIM_TANK_band.mesh"/>
        <CastShadow value="true"/>
