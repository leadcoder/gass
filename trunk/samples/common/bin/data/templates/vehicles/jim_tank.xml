<?xml version="1.0" encoding="UTF-8"?>
<Templates>
  <SceneObjectTemplate>
    <Name value="JimTank"/>
    <Components>
      <LocationComponent>
        <AttachToParent value="1"/>
      </LocationComponent>
      <MeshComponent>
        <Filename value="JIM_TANK_hull.mesh"/>
        <CastShadow value="1"/>
      </MeshComponent>
      <PhysicsGeometryComponent>
        <GeometryType value="box"/>
      </PhysicsGeometryComponent>
      <PhysicsBodyComponent>
        <Mass value="1.0"/>
      </PhysicsBodyComponent>
      <OpenALSoundComponent>
        <Name value="EngineSound"/>
        <SoundFile value="sounds/vehicles/tank/engine.wav"/>
        <Loop value="1"/>
        <MinDistance value="5"/>
        <RolloffFactor value="1"/>
      </OpenALSoundComponent>
      <ParticleSystemComponent>
        <Name value="EngineExhaustFumes"/>
        <ParticleTemplate value="ExhaustFumes"/>
      </ParticleSystemComponent>
      <VehicleEngineComponent>
        <Wheels value="JimTankWheelR1 JimTankWheelR2 JimTankWheelR3 JimTankWheelR4 JimTankWheelR5 JimTankWheelR6 JimTankWheelL1 JimTankWheelL2 JimTankWheelL3 JimTankWheelL4 JimTankWheelL5 JimTankWheelL6"/>
        <EngineType value="tank"/>
        <Power value="0.01"/>
        <Automatic  value="1"/>
        <BrakeTorque  value="5"/>
        <DeclutchTimeChangeGear value="1.0"/>
        <ClutchTimeChangeGear value="1.0"/>
        <MaxRPM value="4000"/>
        <MinRPM value="500"/>
        <RPMGearChangeUp value="3000"/>
        <RPMGearChangeDown value="1500"/>
        <!--<GearRatio value="-13.42 -16.42 0 16.42 12.15 11.52 11.17"/>-->
        <GearRatio value="-13.42 -16.42 0 16.42 13.15 10.52 7.17"/>
        <TurnForce value="100"/>
      </VehicleEngineComponent>
      <InputHandlerComponent>
        <ControlSetting value ="VehicleEngineInputSettings"/>
      </InputHandlerComponent>
      <ArmorComponent>
        <Armor value="50"/>
        <DamageMesh value="JIM_TANK_hull_demolished.mesh"/>
      </ArmorComponent>
      <NetworkMasterComponent>
      </NetworkMasterComponent>
      <LocationTransferComponent>
      </LocationTransferComponent>
      <InputTransferComponent>
        <ControlSetting value ="VehicleEngineInputSettings"/>
      </InputTransferComponent>
    </Components>
      <ComponentContainers>
        <SceneObjectTemplate>
          <Name value="JimTankDriverCameraObject"/>
          <Inheritance value ="JimTankCameraObject"/>
          <Components>
            <LocationComponent>
              <AttachToParent value ="1"/>
              <Position value="-1.0 1.0 -1.0"/>
              <Rotation value="0 0 0"/>
            </LocationComponent>
          </Components>
        </SceneObjectTemplate>
        <SceneObjectTemplate>
          <Name value="JimTankTurret"/> 
          <Components>
            <LocationComponent>
              <AttachToParent value="0"/>
              <Position value="0.0 0.45 0.7"/>
            </LocationComponent>
            <MeshComponent>
              <Filename value="turret.mesh"/>
              <CastShadow value="1"/>
            </MeshComponent>
            <TurretComponent>
            </TurretComponent>
            <PhysicsGeometryComponent>
              <GeometryType value="box"/>
            </PhysicsGeometryComponent>
            <PhysicsBodyComponent>
              <Mass value="0.1"/>
            </PhysicsBodyComponent>
            <PhysicsHingeComponent>
              <Axis value ="0 1 0"/>
              <HighStop value ="-1"/>
              <LowStop value ="1"/>
            </PhysicsHingeComponent>
            <InputHandlerComponent>
              <ControlSetting value="TurretInputSettings"/>
            </InputHandlerComponent>
            <WeaponSystemComponent>
              <ProjectileTemplate value="JimTankMainGunProjectileObject"/>
              <ProjectileStartOffset value="5.0"/>
              <ProjectileStartVelocity value="500"/>
              <NumberOfMagazines value="90"/>
              <MagazineSize value="5"/>
              <RecoilForce value="0 0 100"/>
              <ReloadTime value="3"/>
            </WeaponSystemComponent>
            <ArmorComponent>
              <Armor value="50"/>
              <DamageMesh value="turret_demolished.mesh"/>
            </ArmorComponent>
            <NetworkChildComponent>
            </NetworkChildComponent>
            <LocationTransferComponent>
              <RelativeToParent value="1"/>
            </LocationTransferComponent>
            <InputTransferComponent>
              <ControlSetting value ="TurretInputSettings"/>
            </InputTransferComponent>
          </Components>
          <ComponentContainers>
            <SceneObjectTemplate>
              <Name value="FireSound"/>
              <Components>
                <LocationComponent>
                  <AttachToParent value="1"/>
                  <Position value="-0.05 0.4 -0.45"/>
                </LocationComponent>
                <OpenALSoundComponent>
                  <SoundFile value="sounds/vehicles/tank/cannon.wav"/>
                  <Loop value="0"/>
                  <MinDistance value="5"/>
                  <RolloffFactor value="1"/>
                </OpenALSoundComponent>
              </Components>
            </SceneObjectTemplate>
            <SceneObjectTemplate>
              <Name value="JimTankBarel"/>
              <Components>
                <LocationComponent>
                  <AttachToParent value="1"/>
                  <Position value="0.0 0.3 -1.15"/>
                  <Rotation value="0.0 0.0 0.0"/>
                </LocationComponent>
                <MeshComponent>
                  <Filename value="canon_bas.mesh"/>
                  <CastShadow value="1"/>
                </MeshComponent>
                <ArmorComponent>
                  <Armor value="50"/>
                  <DamageMesh value="canon_bas_demolished.mesh"/>
                </ArmorComponent>
              </Components>
            </SceneObjectTemplate>
          </ComponentContainers>
        </SceneObjectTemplate>
        <SceneObjectTemplate>
          <Name value="JimTankTrackL"/>
          <Inheritance value ="JimTankTrack"/>
          <Components>
            <TrackComponent>
              <DriveWheel value="JimTankWheelL5"/>
              <AnimationSpeedFactor value="0.01 0"/>
            </TrackComponent>
            <LocationComponent>
              <Position value="1.2 0.0 0.0"/>
              <AttachToParent value="1"/>
            </LocationComponent>
          </Components>
        </SceneObjectTemplate>
        <SceneObjectTemplate>
          <Name value="JimTankTrackR"/>
          <Inheritance value ="JimTankTrack"/>
          <Components>
            <TrackComponent>
              <DriveWheel value="JimTankWheelR5"/>
              <AnimationSpeedFactor value="0.01 0"/>
            </TrackComponent>
            <LocationComponent>
              <Position value="-1.2 0.0 0.0"/>
              <AttachToParent value="1"/>
            </LocationComponent>
          </Components>
        </SceneObjectTemplate>
        <SceneObjectTemplate>
          <Name value="JimTankWheelL1"/>
          <Inheritance value ="JimTankWheelL"/>
          <Components>
            <LocationComponent>
              <Position value="1.2 -0.6 1.6"/>
            </LocationComponent>
          </Components>
        </SceneObjectTemplate>
        <SceneObjectTemplate>
          <Name value="JimTankWheelL2"/>
          <Inheritance value ="JimTankWheelL"/>
          <Components>
            <LocationComponent>
              <Position value="1.2 -0.6 0.95"/>
            </LocationComponent>
          </Components>
        </SceneObjectTemplate>
        <SceneObjectTemplate>
          <Name value="JimTankWheelL3"/>
          <Inheritance value ="JimTankWheelL"/>
          <Components>
            <LocationComponent>
              <Position value="1.2 -0.6 0.3"/>
            </LocationComponent>
          </Components>
        </SceneObjectTemplate>
        <SceneObjectTemplate>
          <Name value="JimTankWheelL4"/>
          <Inheritance value ="JimTankWheelL"/>
          <Components>
            <LocationComponent>
              <Position value="1.2 -0.6 -0.35"/>
            </LocationComponent>
          </Components>
        </SceneObjectTemplate>
        <SceneObjectTemplate>
          <Name value="JimTankWheelL5"/>
          <Inheritance value ="JimTankWheelL"/>
          <Components>
            <LocationComponent>
              <Position value="1.2 -0.6 -1.0"/>
            </LocationComponent>
          </Components>
        </SceneObjectTemplate>
        <SceneObjectTemplate>
          <Name value="JimTankWheelL6"/>
          <Inheritance value ="JimTankWheelL"/>
          <Components>
            <LocationComponent>
              <Position value="1.2 -0.6 -1.6"/>
            </LocationComponent>
          </Components>
        </SceneObjectTemplate>
        <SceneObjectTemplate>
          <Name value="JimTankWheelR1"/>
          <Inheritance value ="JimTankWheelR"/>
          <Components>
            <LocationComponent>
              <Position value="-1.2 -0.6 1.6"/>
            </LocationComponent>
          </Components>
        </SceneObjectTemplate>
        <SceneObjectTemplate>
          <Name value="JimTankWheelR2"/>
          <Inheritance value ="JimTankWheelR"/>
          <Components>
            <LocationComponent>
              <Position value="-1.2 -0.6 0.95"/>
            </LocationComponent>
          </Components>
        </SceneObjectTemplate>
        <SceneObjectTemplate>
          <Name value="JimTankWheelR3"/>
          <Inheritance value ="JimTankWheelR"/>
          <Components>
            <LocationComponent>
              <Position value="-1.2 -0.6 0.3"/>
            </LocationComponent>
          </Components>
        </SceneObjectTemplate>
        <SceneObjectTemplate>
          <Name value="JimTankWheelR4"/>
          <Inheritance value ="JimTankWheelR"/>
          <Components>
            <LocationComponent>
              <Position value="-1.2 -0.6 -0.35"/>
            </LocationComponent>
          </Components>
        </SceneObjectTemplate>
        <SceneObjectTemplate>
          <Name value="JimTankWheelR5"/>
          <Inheritance value ="JimTankWheelR"/>
          <Components>
            <LocationComponent>
              <Position value="-1.2 -0.6 -1.0"/>
            </LocationComponent>
          </Components>
        </SceneObjectTemplate>
        <SceneObjectTemplate>
          <Name value="JimTankWheelR6"/>
          <Inheritance value ="JimTankWheelR"/>
          <Components>
            <LocationComponent>
              <Position value="-1.2 -0.6 -1.6"/>
            </LocationComponent>
          </Components>
        </SceneObjectTemplate>
      </ComponentContainers>
    </SceneObjectTemplate>
  <SceneObjectTemplate>
    <Name value="JimTankWheel"/>
    <Components>
      <LocationComponent>
      </LocationComponent>
      <PhysicsGeometryComponent>
        <GeometryType value="sphere"/>
        <Friction value="1.0"/>
      </PhysicsGeometryComponent>
      <PhysicsBodyComponent>
        <GeometryType value="sphere"/>
        <Mass value="0.02"/>
      </PhysicsBodyComponent>
      <PhysicsSuspensionComponent>
        <Strength value="0.1"/>
        <Damping value="4"/>
        <SwayForce value="100"/>
      </PhysicsSuspensionComponent>
    </Components>
  </SceneObjectTemplate>
  <SceneObjectTemplate>
    <Name value="JimTankWheelL"/>
    <Inheritance value ="JimTankWheel"/>
    <Components>
      <MeshComponent>
        <Filename value="wheel_l.mesh"/>
        <CastShadow value="true"/>
      </MeshComponent>
    </Components>
  </SceneObjectTemplate>
  <SceneObjectTemplate>
    <Name value="JimTankWheelR"/>
    <Inheritance value ="JimTankWheel"/>
    <Components>
      <MeshComponent>
