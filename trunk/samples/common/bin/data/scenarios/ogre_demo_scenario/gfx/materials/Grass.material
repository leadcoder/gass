import * from  "terrain.material"
material FadeGrass : VegetationTemplate
{ 
	//receive_shadows off 
	technique 
	{ 
		pass 
		{ 
			scene_blend alpha_blend
			alpha_rejection greater 10 
			ambient 1 1 1 1 
			specular 0.0 0.0 0.0 1 10 
			emissive 0.0 1.0 0.0 0.5 
			depth_write off

			texture_unit 
			{ 
				texture gfx/materials/grass02.dds 
			}
			
		} 

	} 
} 