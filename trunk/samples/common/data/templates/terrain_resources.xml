  <SceneObjectTemplate>
    <Name value="LightObject"/>
	<Instantiable value= "true"/>
    <Components>
      <LightComponent>
        <LightType value="LT_DIRECTIONAL"/>
        <CastShadow value="1"/>
      </LightComponent>
      <LocationComponent>
        <Position value="0 0 0"/>
        <Rotation value="45 45 0"/>
      </LocationComponent>
    </Components>
  </SceneObjectTemplate>
  <SceneObjectTemplate>
    <Name value="GrassVegetationObject"/>
	<Instantiable value= "true"/>
    <Components>
      <GrassLoaderComponent>
	    <Name value = "GRASS_LOADER"/>
        <PageSize value = "45"/>
        <ViewDistance value ="100"/>
        <DensityMap value= ""/>
        <!--<ColorMap value= "grass_color.png"/>-->
      </GrassLoaderComponent>
      <GrassLayerComponent>
        <Name value = "FAR_LAYER"/>
        <Material value="DefaultGrass"/>
        <DensityFactor value= "1"/>
        <MaxSize value="2.0 0.7"/>
        <MinSize value="1.5 0.5"/>
        <EnableSway value="true"/>
        <SwaySpeed value="0.1"/>
        <SwayLength value="0.1"/>
        <SwayDistribution value="10"/>
        <FadeTechnique value ="FADETECH_ALPHA"/>
        <RenderTechnique value ="GRASSTECH_QUAD"/>
        <BlendWithGround value="false"/>
      </GrassLayerComponent>
    </Components>
  </SceneObjectTemplate>
  <SceneObjectTemplate>
    <Name value="FirVegetationObject"/>
	<Instantiable value="true"/>
    <Components>
      <DensityMapComponent/>
      <TreeGeometryComponent>
        <Name value="Fir1"/>
        <Mesh value= "fir05_30.mesh"/>
        <ImposterAlphaRejectionValue value ="140"/>
        <DensityFactor value= "0.004"/>
        <MeshDistance value ="170"/>
        <PageSize value ="50"/>
        <MeshFadeDistance value ="0"/>
        <ImposterDistance value ="300"/>
        <ImposterFadeDistance value ="40"/>
        <SetHeightAtStartup value = "true"/>
        <MaxMinScale value ="0.05 0.1"/>
        <CastShadows value="true"/>
      </TreeGeometryComponent>
    </Components>
  </SceneObjectTemplate>
   <SceneObjectTemplate>
