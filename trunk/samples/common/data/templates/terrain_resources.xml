      <ManualMeshComponent>
      </ManualMeshComponent>
      <OgreRoadComponent>
	  </OgreRoadComponent>
    </Components>
  </SceneObjectTemplate>
  <SceneObjectTemplate>
    <Name value="RoadWaypoint"/>
    <Components>
      <WaypointComponent>
      </WaypointComponent>
      <LocationComponent>
        <Position value="0 0 0"/>
        <Rotation value="0 0 0"/>
        <AttachToParent value="1"/>
      </LocationComponent>
      <!--<BoxGeometryComponent>
		<Size value="0.2 0.2 0.2"/>
	  </BoxGeometryComponent>-->
      <BillboardComponent>
        <Name value="NodeGeom"/>
        <Material value="node.dds"/>
        <Width value="0.4"/>
        <Height value="0.4"/>
      </BillboardComponent>
      <ManualMeshComponent>
        <Name value="TangentGeom"/>
      </ManualMeshComponent>
      <PhysicsBoxGeometryComponent/>
    </Components>
    <ComponentContainers>
      <SceneObjectTemplate>
        <Name value="Tangent"/>
        <Components>
          <LocationComponent>
            <Position value="0 0 2"/>
            <Rotation value="0 0 0"/>
            <AttachToParent value="1"/>
          </LocationComponent>
          <BoxGeometryComponent>
            <Size value="0.1 0.1 0.1"/>
          </BoxGeometryComponent>
          <ManualMeshComponent>
          </ManualMeshComponent>
          <PhysicsBoxGeometryComponent/>
        </Components>
        <ComponentContainers>
        </ComponentContainers>
      </SceneObjectTemplate>
    </ComponentContainers>
  </SceneObjectTemplate>
  <SceneObjectTemplate>
    <Name value="PaintGizmo"/>
	<Serialize value="false"/>
