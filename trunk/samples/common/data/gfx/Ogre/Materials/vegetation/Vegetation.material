 import * from  "terrain.material"	 

material DefaultGrass : VegetationTemplate
{ 
	//receive_shadows off 
	technique light
	{ 
		pass 
		{ 
			alpha_rejection greater 128 
			ambient 1 1 1 1 
			specular 0.0 0.0 0.0 1 10 
			emissive 0.0 1.0 0.0 0.5 

			texture_unit diffuse_map
			{ 
				texture veg_default.dds 
			}
		} 

	} 
} 

