import * from  "paged_geometry.material"	
material DefaultGrass : PGGrassTemplate
{ 
	set_texture_alias BaseTexture veg_default.dds
	set	$AmbientColor "0.5 0.55 0.5 1"
	set $DiffuseColor "0.5 0.55 0.5 1"
    set $SpecularColor "0 0 0"
    set $Shininess "128"
	set $AlphaRejection "greater 128"
} 