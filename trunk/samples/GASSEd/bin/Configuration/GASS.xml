    </Load>
</SceneObjectTemplateManager>
<Systems>
  <EditorSystem>
 	<TaskNode value="PRE_SIM"/> 
  </EditorSystem>
 <OISInputSystem>
 	<TaskNode value="PRE_SIM"/> 
	<ExclusiveMode value ="0"/>
	<EnableMouse value="false"/>
  </OISInputSystem>
  <ControlSettingsSystem>
	<TaskNode value="PRE_SIM"/>
  </ControlSettingsSystem>
  <CoreSystem>
	<TaskNode value="PRE_SIM"/>
  </CoreSystem>
  <MyGUISystem>
	<TaskNode value="PRE_SIM"/>
  </MyGUISystem>  
  <GameSystem>
	<TaskNode value="SIM"/>
  </GameSystem>
  <RakNetNetworkSystem>
    <Name value = "NetworkSystem"/>
	<TaskNode value="SIM"/>
  </RakNetNetworkSystem>
  <OpenALSoundSystem>
    <Name value = "SoundSystem"/>
	<TaskNode value="PRE_SIM"/>
  </OpenALSoundSystem>
  <CEGUISystem>
    <Name value = "GUISystem"/>
	<TaskNode value="PRE_SIM"/>
  </CEGUISystem>
  <LuaScriptSystem>
    <Name value = "GUISystem"/>
	<TaskNode value="PRE_SIM"/>
  </LuaScriptSystem>
  <PhysXPhysicsSystem>
    <Name value = "PhysicsSystem"/>
