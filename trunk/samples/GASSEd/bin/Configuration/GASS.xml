	<!--<AddResourceLocation Path="%GASS_DATA_HOME%/gfx/ogre/particles/" Group="GASS" Type="FileSystem" Recursive="false"/>-->
    
    <AddResourceLocation Path="%GASS_DATA_HOME%/gfx/textures/misc" Group="GASS" Type="FileSystem" Recursive="false"/>
