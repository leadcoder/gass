    <Plugin value="Plugin_BSPSceneManager"/>
    <Plugin value="Plugin_OctreeSceneManager"/>
    <Plugin value="Plugin_ParticleFX"/>
    <TaskGroup value="MAIN_TASK_GROUP"/>
    <PostFilters value="HDR"/>
	<TaskNode value="POST_SIM"/>
  </OgreGraphicsSystem>
  <OgreResourceSystem>
  <AddResourceLocation Path="%GASS_DATA_HOME%/gfx/ogre/ExternalResources/Ogre/OgreCore.zip" Group="Bootstrap" Type="Zip" Recursive="false"/>
    <!--<AddResourceLocation Path="%GASS_DATA_HOME%/gfx/ogre/ExternalResources/Ogre/bguires.zip" Group="Bootstrap" Type="Zip" Recursive="false"/>
    <AddResourceLocation Path="%GASS_DATA_HOME%/gfx/ogre/ExternalResources/SkyX" Group="SkyX" Type="FileSystem" Recursive="true"/>
