    <AddResourceLocation Path="%GASS_DATA_HOME%/gfx/ogre/ExternalResources/ogre/materials/scripts" Group="Ogre" Type="FileSystem" Recursive="true"/>
    <AddResourceLocation Path="%GASS_DATA_HOME%/gfx/ogre/ExternalResources/ogre/materials/textures" Group="Ogre" Type="FileSystem" Recursive="true"/>!-->
